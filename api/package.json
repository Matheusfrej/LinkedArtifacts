{
  "name": "linkedartifacts-api",
  "version": "0.1.0",
  "description": "linkedartifacts-api",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node-dev --respawn --no-notify --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "drizzle:generate": "npx drizzle-kit generate",
    "drizzle:migrate": "npx drizzle-kit migrate",
    "drizzle:studio": "npx drizzle-kit studio",
    "prepare": "echo \"Run npm install in /api to install dependencies\""
  },
  "keywords": [
    "express",
    "typescript",
    "drizzle-orm",
    "ddd"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.3.1",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "pg": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.9",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
